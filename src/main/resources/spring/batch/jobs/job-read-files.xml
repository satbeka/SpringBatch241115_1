<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:batch="http://www.springframework.org/schema/batch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/batch 
		http://www.springframework.org/schema/batch/spring-batch-2.2.xsd
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		">

	<import resource="../config/context.xml" />

	<bean id="domain" class="kz.springappsat.batch.Domain" />

	<bean id="cloneTblToTbl2Tasklet" class="kz.springappsat.batch.tasklet.CloneTblToTbl2Tasklet" >
		<property name="tblName" value="TISR_TWLTRAINING" />
	</bean>
	
	<job id="cloneJob" xmlns="http://www.springframework.org/schema/batch">
		<step id="step1">
		    <tasklet ref="cloneTblToTbl2Tasklet" />
		</step>
	</job>



</beans>